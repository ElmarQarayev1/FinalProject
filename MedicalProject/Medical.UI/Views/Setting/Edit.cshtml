@model SettingEditRequest
@{
    ViewData["Title"] = "Edit";
}

<!-- Begin Page Content -->
<div class="container-fluid">
    <!-- Page Heading -->
    <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800">Settings</h1>
    </div>
    <div class="row">
        <div class="col-md-8 mx-auto">
            <form class="user" asp-action="Edit" asp-route-key="@ViewBag.Key" method="post">
                <div class="form-group">
                    <label for="noInput">Value</label>
                    <input asp-for="Value" class="form-control" id="noInput" placeholder="" style="background-color:white;">
                    <span class="text-danger" asp-validation-for="Value"></span>
                </div>
              
                <button class="btn btn-primary" type="submit">
                    Edit Setting
                </button>
            </form>
        </div>
    </div>
</div>
@section scripts {
    @Html.Partial("_ValidationScriptsPartial");
}
